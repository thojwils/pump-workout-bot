{
    "name": "discord-bot",
    "type": "commonjs",
    "version": "1.1.0",
    "description": "The 1 Stop Shop to get your Pump!",
    "main": "index.js",
    "scripts": {
            "start": "node index.js",
            "deploy": "node deploy-commands.js",
            "test": "jest --detectOpenHandles --forceExit",
            "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
            "test:safe": "node run-tests.js",
            "test:report": "node run-tests.js --open-report",
            "test:loop": "node test-loop.js",
            "test:simple": "node test-loop-simple.js",
            "fix-tests": "node fix-tests.js",
            "test:db": "cross-env NODE_ENV=test node tests/postgres-tests.js",
            "test:commands": "jest tests/commands/",
            "test:unit": "jest",
            "test:ci": "jest --ci",
            "migrate": "node db/migration.js",
            "create-schema": "node db/create-schema.js",
            "update-schema": "node db/update-schema.js",
            "test-connection": "node tests/test-connection.js",
            "test-user-data": "node tests/test-user-data.js",
            "simulate:game": "jest tests/simulate-logchop.js",
            "simulate:edge": "jest tests/simulate-logchop.js edge-cases",
            "test:reliable": "node run-tests.js --coverage"
        },
    "dependencies": {
        "axios": "^1.6.1",
        "canvas": "^2.11.2",
        "discord.js": "^14.13.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "lru-cache": "^10.0.0",
        "moment": "^2.29.4", 
        "npm": "^9.4.2",
        "openai": "^4.17.4",
        "pg": "^8.16.3"
    },
    "engines": {
        "node": "^18.14.0 || ^18.17.1"
    },
    "author": "TJ Wilson",
    "license": "",
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "cross-env": "^10.0.0",
        "jest": "^29.7.0",
        "jest-html-reporter": "^3.10.2"
    }
}